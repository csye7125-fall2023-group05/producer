{
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": false
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:prettier/recommended",
    "eslint:recommended"
  ],
  "overrides": [
    {
      "env": {
        "node": true
      },
      "files": [".eslintrc.{js,cjs}"],
      "parserOptions": {
        "sourceType": "script"
      }
    }
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off",
    "semi": "off",
    "comma-dangle": "off",
    "no-shadow": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "consistent-return": "off",
    "max-classes-per-file": "off",
    "default-param-last": "off",
    "no-undef": "off"
  }
}
